{
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true,
    "useTabs": false,
    "tabWidth": 2,
    "printWidth": 100,
    "arrowParens": "always",
    "plugins": [
        "prettier-plugin-tailwindcss"
    ],
    "tailwindFunctions": [
        "clsx",
        "cn",
        "cva",
        "tw"
    ],
    "endOfLine": "auto"
}